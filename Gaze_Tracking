gaze = GazeTracking()
gaze.refresh(frame)
        new_frame = gaze.annotated_frame()
        text = ""
        if gaze.is_right():
            text = "Looking right"
        elif gaze.is_left():
            text = "Looking left"
        elif gaze.is_center():
            text = "Looking center"
        elif gaze.is_blinking():
            text = "Blinking"

        cv2.putText(new_frame, text, (60, 60), cv2.FONT_HERSHEY_DUPLEX, 2, (255, 0, 0), 2)
        print(text)
